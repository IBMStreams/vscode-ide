    "grammars": [
      {
        "language": "spl",
        "scopeName": "source.spl",
        "path": "./syntaxes/spl.tmLanguage.json"
      },
      {
        "language": "spl-log",
        "scopeName": "source.splLog",
        "path": "./syntaxes/spl-log.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "spl",
        "aliases": [
          "SPL",
          "spl"
        ],
        "extensions": [
          ".spl"
        ],
        "configuration": "./language-configuration.json"
      },
