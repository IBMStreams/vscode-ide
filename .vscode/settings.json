{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.enable": true,
  "eslint.packageManager": "yarn",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.trimTrailingWhitespace": true,
  "npm.packageManager": "yarn",
  "search.exclude": {
    "**/dist": true,
    "**/out": true
  }
}
