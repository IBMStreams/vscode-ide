{
    "compilerOptions": {
        "allowJs": true,
        "alwaysStrict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "noUnusedLocals": true,
        "outDir": "out",
        "resolveJsonModule": true,
        "sourceMap": true,
        "target": "es6",
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules",
        "dist",
        "out",
        ".vscode-test"
    ]
}